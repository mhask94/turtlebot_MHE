<?xml version="1.0"?>

<launch>

  <arg name="bag_path" default="$(env BAG_PATH)"/>

  <!-- MHE Estimator -->
  <node name="mhe_estimator" pkg="turtlebot_MHE" type="mhe_node.py" output="screen" />

  <!-- ROS Bag -->
  <node name="player" pkg="rosbag" type="play" output="screen" args="--clock $(arg bag_path)"/>

</launch>
